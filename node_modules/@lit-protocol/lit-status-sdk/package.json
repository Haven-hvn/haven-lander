{
  "name": "@lit-protocol/lit-status-sdk",
      "version": "0.1.8",
  "description": "SDK for interacting with Lit Status monitoring server",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && npm run dev:setup 2>/dev/null || true && tsc --project tsconfig.build.json && bun build ./src/sdk/index.ts --outdir dist --format esm --sourcemap --minify",
    "dev:setup": "prisma generate",
    "gen:read-key": "bun run scripts/APIKeyManager/generate-read-key.ts",
    "gen:write-key": "bun run scripts/APIKeyManager/generate-write-key.ts",
    "build:docs": "cd ./docs && bun run build",
    "dev:docs": "cd ./docs && bun run dev",
    "start:backend": "bun run ./src/server/server.ts",
    "start:app": "bun run ./apps/lit-status-page/server.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.3",
    "prisma": "^6.13.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@google-cloud/monitoring": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
        "prom-client": "^15.1.3"
  },
  "optionalDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.203.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
